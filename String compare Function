#include <iostream>
#include <vector>
#include <iomanip>
#include <string.h>
#include <algorithm>

using namespace std;
int main(){
     char str[50], str1[50];
     cout << "Enter the string: ";
     gets(str);
     cout << "Enter the string1: ";
     gets(str1);

     cout << str << " " << str1 << "\n";
    if (strcmp(str, str1) == 0){
        cout << "String is equal!";
    }
    else {
        cout << "String is not equal!";
    }
}
