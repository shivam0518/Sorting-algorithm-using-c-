// finding least significant digit and cheack wheather it is giving correct output as expected or not.


#include <bits/stdc++.h>
using namespace std;

char upper(char c){
     return 'A' + c - 'a';
}

int main(){
    int t;
    cin >> t;
   while (t--){
       int n;
       cin >> n;
       long long product = 1;
       int x;
       while(n--){
          cin >> x;
          product *= x;
       }
       int digit = product % 10;
       if (digit == 2 || digit == 3 || digit == 5){
        cout << "YES\n";
       }
       else {
        cout << "NO\n";
       }
   }
}
